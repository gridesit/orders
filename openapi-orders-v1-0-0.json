openapi: 3.0.0
info:
  title: Orders
  description: Endpoints for managing orders.
  contact: {}
  version: '1.0.0'
servers:
- url: http://example.com
  variables: {}
paths:
  /orders:
    get:
      tags:
      - Orders
      summary: All
      description: Get all orders in the system, optionally narrowing by unix timestamps.
      operationId: All
      parameters:
      - name: start
        in: query
        description: A starting UNIX timestamp.
        required: true
        style: form
        explode: true
        schema:
          type: string
          example: '{{$timestamp}}'
      - name: end
        in: query
        description: An ending UNIX timestamp.
        required: true
        style: form
        explode: true
        schema:
          type: string
          example: '{{$timestamp}}'
      responses:
        200:
          description: ''
          headers: {}
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/All'
              example:
                orders:
                - id: 1550861260442-MTU1MDg2MTI2MDQ0Mg==
                  items:
                  - id: XYZ-JEAN-123
                    quantity: 2
      deprecated: false
  /orders/return:
    get:
      tags:
      - Orders
      - Actions
      summary: Return
      description: Initiate a return. The order id persists for futher reference.
      operationId: Return
      parameters:
      - name: id
        in: query
        description: The order ID to return.
        required: true
        style: form
        explode: true
        schema:
          type: string
          example: 1550861260442-MTU1MDg2MTI2MDQ0Mg==
      responses:
        200:
          description: ''
          headers: {}
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/Return'
              example:
                status: success
      deprecated: false
components:
  schemas:
    All:
      title: All
      required:
      - orders
      type: object
      properties:
        orders:
          type: array
          items:
            $ref: '#/components/schemas/Order'
          description: ''
      example:
        orders:
        - id: 1550861260442-MTU1MDg2MTI2MDQ0Mg==
          items:
          - id: XYZ-JEAN-123
            quantity: 2
    Order:
      title: Order
      required:
      - id
      - items
      type: object
      properties:
        id:
          type: string
        items:
          type: array
          items:
            $ref: '#/components/schemas/Item'
          description: ''
      example:
        id: 1550861260442-MTU1MDg2MTI2MDQ0Mg==
        items:
        - id: XYZ-JEAN-123
          quantity: 2
    Item:
      title: Item
      required:
      - id
      - quantity
      type: object
      properties:
        id:
          type: string
        quantity:
          type: integer
          format: int32
      example:
        id: XYZ-JEAN-123
        quantity: 2
    Foruser:
      title: Foruser
      required:
      - orders
      type: object
      properties:
        orders:
          type: array
          items:
            $ref: '#/components/schemas/Order'
          description: ''
      example:
        orders:
        - id: 1550861260442-MTU1MDg2MTI2MDQ0Mg==
          items:
          - id: XYZ-JEAN-123
            quantity: 2
    Return:
      title: Return
      required:
      - status
      type: object
      properties:
        status:
          type: string
      example:
        status: success
tags:
- name: Get
  description: Requests specific to retrieving placed orders.
- name: Actions
  description: Requests specific to modifying orders.